{
	"env": {
		"node": true
	},
	"parserOptions": {
		"ecmaVersion": 6
	},
	"globals": {},
	"extends": "standard",
	"rules": {
		"camelcase": 0,
		"consistent-return": 0,
		"indent": ["error", "tab"],
		"max-len": ["error", { "code": 120 }],
		"no-else-return": 1,
		"no-tabs": "off",
		"no-var": "off",
		"one-var": ["error", { "var": "never" }],
		"semi": [1, "always"],
		"space-unary-ops": 2
	}
}
